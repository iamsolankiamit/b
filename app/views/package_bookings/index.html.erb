<h1>Listing package_bookings</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>Unit count</th>
    <th>Package</th>
    <th>Total</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @package_bookings.each do |package_booking| %>
  <tr>
    <td><%= package_booking.name %></td>
    <td><%= package_booking.email %></td>
    <td><%= package_booking.phone %></td>
    <td><%= package_booking.unit_count %></td>
    <td><%= package_booking.package_id %></td>
    <td><%= package_booking.total %></td>
    <td><%= link_to 'Show', package_booking %></td>
    <td><%= link_to 'Edit', edit_package_booking_path(package_booking) %></td>
    <td><%= link_to 'Destroy', package_booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

