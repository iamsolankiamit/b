<% content_for :body do %>
  <div class="container">
  <div class="row">
    <div class="col-sm-9">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="text-center">List your space</h3>
          <h4 class="text-center">Earn extra money by renting out a spare room, apartment, cottage or holiday home.</h4>
        </div>
        <div class="panel-body">
          <%= render :partial => 'new_form' %>
        </div>
      </div>
    </div>
    <div class="col-sm-3">
      <%= render "help/why_list_sidebar"  %>
    </div>
  </div>
</div>
<% end %>
<% content_for :js do %>
  <script type="text/javascript" charset="utf-8">
$("#offer_city").geocomplete({
details: "#new_offer",
detailsAttribute: "data-geo"
});
</script>
<% end %>
